# Notion Integration Configuration
# Copy this file to .env and fill in your values

# Your Notion integration token
# Get this from https://www.notion.so/my-integrations
NOTION_TOKEN=

# Database IDs from Notion
# These are created during the setup process
GOALS_DB_ID=
INITIATIVES_DB_ID=
DELIVERABLES_DB_ID=

# API authentication key - share this with authorized users
API_KEY=
